BASE <http://example.com/base/>\nPREFIX mapper: <http://www.ontotext.com/mapper/>\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\n\nCONSTRUCT {\n    ?s1 <as> ?o_as .\n} WHERE {\n    SERVICE <rdf-mapper:ontorefine:1618605512877> {\n        # Columns as variables:\n        #   ?c_color, ?c_director_name, ?c_num_critic_for_reviews, ?c_duration, ?c_director_facebook_likes,\n        #   ?c_actor_3_facebook_likes, ?c_actor_2_name, ?c_actor_1_facebook_likes,\n        #   ?c_gross, ?c_genres, ?c_actor_1_name, ?c_movie_title, ?c_num_voted_users,\n        #   ?c_cast_total_facebook_likes, ?c_actor_3_name, ?c_facenumber_in_poster,\n        #   ?c_plot_keywords, ?c_movie_imdb_link, ?c_num_user_for_reviews, ?c_language,\n        #   ?c_country, ?c_content_rating, ?c_budget, ?c_title_year, ?c_actor_2_facebook_likes,\n        #   ?c_imdb_score, ?c_aspect_ratio, ?c_movie_facebook_likes\n        # Metadata as variables:\n        #   ?row_index, ?record_id\n        BIND(IRI(mapper:encode_iri(?c_duration)) as ?s1)\n        BIND(STR(?c_color) as ?o_as)\n    }\n}
