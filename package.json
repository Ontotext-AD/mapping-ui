{
  "name": "mapping-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod && mv dist/mapping-ui/assets/i18n/en.json dist/mapping-ui/assets/i18n/en.js",
    "test": "npx jest",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "start:ci": "http-server ./dist/mapping-ui -a localhost -p 4200 -c-1",
    "cy:ci": "ng build --prod && mv dist/mapping-ui/assets/i18n/en.json dist/mapping-ui/assets/i18n/en.js && run-p --race start:ci cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec cypress/test/**/*.spec.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.6",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "^9.1.6",
    "@angular/compiler": "^9.1.6",
    "@angular/core": "^9.1.6",
    "@angular/flex-layout": "^9.0.0-beta.30",
    "@angular/forms": "^9.1.6",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "^9.1.6",
    "@angular/platform-browser-dynamic": "^9.1.6",
    "@angular/router": "^9.1.6",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@w11k/ngx-componentdestroyed": "^5.0.2",
    "rxjs": "^6.5.4",
    "class-transformer": "^0.2.3",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "^9.1.5",
    "@angular/compiler-cli": "^9.1.6",
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "codelyzer": "^5.1.2",
    "cypress": "^4.5.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-html": "^6.0.2",
    "http-server": "^0.12.3",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^26.0.1",
    "jest-preset-angular": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  }
}
