{
  "name": "rdf-mapping-editor",
  "version": "1.0.0-TR16",
  "description": "A webrun component to create a table to RDF data mapping.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href=/orefine/extension/mapping-editor/ && mv dist/rdf-mapping-editor/assets/i18n/en.json dist/rdf-mapping-editor/assets/i18n/en.js",
    "test": "npx jest",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "start:ci": "http-server ./dist/rdf-mapping-editor -a localhost -p 4200 -c-1",
    "cy:ci": "ng build --prod && mv dist/rdf-mapping-editor/assets/i18n/en.json dist/rdf-mapping-editor/assets/i18n/en.js && run-p --race start:ci cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec cypress/test/**/*.spec.ts"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ontotext-AD/mapping-ui.git"
  },
