{
  "name": "rdf-mapping-editor",
  "version": "2.0.0",
  "description": "A webrun component to create a table to RDF data mapping.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production && mv dist/rdf-mapping-editor/assets/i18n/en.json dist/rdf-mapping-editor/assets/i18n/en.js",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "start:ci": "http-server ./dist/rdf-mapping-editor -a localhost -p 4200 -c-1",
    "cy:ci": "ng build --configuration=cypress-tests && mv dist/rdf-mapping-editor/assets/i18n/en.json dist/rdf-mapping-editor/assets/i18n/en.js && run-p --race start:ci cy:parallel",
    "cy:open": "cypress open",
    "cy:parallel": "cypress-parallel -s cy:run -t 4 -d cypress/test/**/*.cy.ts",
    "cy:run": "cypress run --spec cypress/test/**/*.cy.ts"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ontotext-AD/mapping-ui.git"
  },
  "keywords": [
    "ontotext",
    "graphdb",
    "mapping",
    "refine"
  ],
  "author": {
    "name": "\"Sirma AI\" JSC, trading as Ontotext",
    "url": "https://www.ontotext.com/"
  },
  "contributors": [
